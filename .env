# URLs des pages à scanner
PAGE_1_URL=https://www.rdv-prefecture.interieur.gouv.fr/rdvpref/reservation/demarche/2381/cgu/
PAGE_2_URL=https://www.rdv-prefecture.interieur.gouv.fr/rdvpref/reservation/demarche/3260/cgu/

# Configuration captcha (2captcha.com ou anticaptcha.com)
CAPTCHA_API_KEY=your_api_key_here
CAPTCHA_SERVICE=2captcha

# Configuration Gemini Vision (recommandé pour résolution multimodale)
# Obtenir votre clé: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here
USE_GEMINI=true

# Configuration de notification (optionnel)
NOTIFICATION_EMAIL=your_email@example.com
NOTIFICATION_WEBHOOK=your_webhook_url_here

# Intervalle de vérification (en secondes)
CHECK_INTERVAL=300

# Mode headless
# IMPORTANT: Sur Mac/Windows, mettez HEADLESS=false pour contourner Cloudflare
# En mode non-headless, vous verrez le navigateur et Cloudflare vous bloquera moins
HEADLESS=false

# Muet l'onglet pour éviter le son du captcha
MUTE_BROWSER=true
